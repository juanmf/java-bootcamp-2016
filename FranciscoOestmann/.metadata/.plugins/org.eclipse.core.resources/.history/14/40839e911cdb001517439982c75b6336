package exercise3;

import java.sql.*;

public class AccesorTwoDAO implements SelectorDAOImpl{

	Connection conn = null;
	private int id;
	
	//Constructor
	public AccesorTwoDAO(int id) {
		super();
		this.id = id;
	}
	
	//Getter and Setter
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public void connect() {
		try{
			Class.forName("com.mysql.jdbc.Driver");
			conn = DriverManager.getConnection("url", "User", "Pass");
			
		}catch(Exception e){
			System.out.println(e);
		}
	}

	@Override
	public void close() {
		try{
			conn.close();
		}catch(Exception e){
			System.out.println(e);
		}
		
	}
}
